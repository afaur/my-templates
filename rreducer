import { THING_SELECTED } from '../actions/action_thing'

const ReducerThing = function(state = null, action) {

  switch(action.type) {
  case THING_SELECTED:
    return [ action.payload.data, ...state ]
    // Same as action.payload.data.concat([state])
  }

  return state

}

export { ReducerThing }
