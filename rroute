import React from 'react'
import { Route, IndexRoute } from 'react-router'

import { App } from './components/app'
import { Index } from './components/index'

const routes = React.createElement(
  Route,
  {
    path: "/",
    component: App,
  },
  React.createElement(
    IndexRoute,
    {
      component: Index,
    }
  ),
/*
  React.createElement(
    Route,
    {
      path: "greet",
      component: Greeting,
    }
  )
*/
)

export { routes }
